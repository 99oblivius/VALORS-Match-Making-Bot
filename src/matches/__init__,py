import threading
from matches import Match

def make_match(bot, guild_id, match_id):
    match = Match(bot, guild_id, match_id)
    threading.Thread(target=match.start).start()

def load_ongoing_matches(bot, guild_id, matches):
    for match in matches:
        match = Match(bot, guild_id, match.id, match.state)
        threading.Thread(target=match.run).start()
